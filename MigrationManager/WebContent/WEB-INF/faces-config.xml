<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<converter>
		<converter-id>pickListConverter</converter-id>
		<converter-class>com.ibm.ecm.mm.util.PickListConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>fileUpload</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.util.FileUpload</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>extractionBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.ExtractionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>identificationBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.IdentificationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>generationBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.GenerationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reportBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.ReportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>validationBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.ValidationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>doctypeBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.DoctypeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>aclextractionBean</managed-bean-name>
		<managed-bean-class>com.ibm.ecm.mm.bean.AclExtractionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
